<h1>Show Order</h1>

<ul>

  <li>
    <strong>Customer:</strong>
    <%= @order.customer %>
  </li>

  <li>
    <strong>Phone:</strong>
    <%= @order.phone %>
  </li>

  <li>
    <strong>Items:</strong>
    <%= for i <- @order.items do i.name end %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.order_path(@conn, :edit, @order) %></span>
<span><%= link "Back", to: Routes.order_path(@conn, :index) %></span>
